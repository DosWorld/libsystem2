all : DOS DPMI

DOS: SYSTEM2.TPU WINCB.TPU
DPMI: SYSTEM2.TPP WINCB.TPP

SYSTEM2.TPU: SYSTEM2.PAS SYSEMS.PAS SYSXMS.PAS SYSMEM.PAS
	tpc /m SYSTEM2.PAS
SYSTEM2.TPP: SYSTEM2.PAS SYSMEM.PAS
	bpc /m /cp SYSTEM2.PAS

WINCB.TPU: WINCB.PAS
	tpc /m WINCB.PAS
WINCB.TPP: WINCB.PAS
	bpc /m /cp WINCB.PAS

clean:
	del *.TPU
	del *.TPP
