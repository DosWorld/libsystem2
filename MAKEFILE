all : DOS DPMI

DOS: SYSTEM2.TPU WINCB.TPU STRBIN.TPU ARGS.TPU DBM.TPU DBB.TPU RC4.TPU ANSI.TPU QSORT.TPU STR.TPU ARRAYS.TPU KMINPUT.TPU LZJB.TPU DETECT.TPU READARC.TPU
DPMI: SYSTEM2.TPP WINCB.TPP STRBIN.TPP ARGS.TPP DBM.TPP DBB.TPP RC4.TPP ANSI.TPP QSORT.TPP STR.TPP ARRAYS.TPP KMINPUT.TPP LZJB.TPP DETECT.TPP READARC.TPP

SYSTEM2.TPU: SYSTEM2.PAS EXMS.PAS EXCACHE.PAS
        tpc /b SYSTEM2.PAS

SYSTEM2.TPP: SYSTEM2.PAS
        bpc /b /cp SYSTEM2.PAS

QSORT.TPU: QSORT.PAS
        tpc /b QSORT.PAS

QSORT.TPP: QSORT.PAS
        bpc /b /cp QSORT.PAS

WINCB.TPU: WINCB.PAS
        tpc /b WINCB.PAS

WINCB.TPP: WINCB.PAS
        bpc /b /cp WINCB.PAS

ARGS.TPU: ARGS.PAS
        tpc /b ARGS.PAS

ARGS.TPP: ARGS.PAS
        bpc /b /cp ARGS.PAS

ANSI.TPU: ANSI.PAS
        tpc /b ANSI.PAS

ANSI.TPP: ANSI.PAS
        bpc /b /cp ANSI.PAS

DBM.TPU: DBM.PAS SYSTEM2.PAS
        tpc /b DBM.PAS

DBM.TPP: DBM.PAS SYSTEM2.PAS
        bpc /b /cp DBM.PAS

RC4.TPU: RC4.PAS
        tpc /b RC4.PAS
RC4.TPP: RC4.PAS
        bpc /b /cp RC4.PAS

STR.TPU: STR.PAS
        tpc /b STR.PAS
STR.TPP: STR.PAS
        bpc /b /cp STR.PAS

LZJB.TPU: LZJB.PAS
        tpc /b LZJB.PAS
LZJB.TPP: LZJB.PAS
        bpc /b /cp LZJB.PAS

DETECT.TPU: DETECT.PAS
        tpc /b DETECT.PAS
DETECT.TPP: DETECT.PAS
        bpc /b /cp DETECT.PAS

STRBIN.TPU: STRBIN.PAS
        tpc /b STRBIN.PAS
STRBIN.TPP: STRBIN.PAS
        bpc /b /cp STRBIN.PAS

ARRAYS.TPU: ARRAYS.PAS STR.TPU
        tpc /b ARRAYS.PAS
ARRAYS.TPP: ARRAYS.PAS STR.TPP
        bpc /b /cp ARRAYS.PAS

KMINPUT.TPU: KMINPUT.PAS
        tpc /b KMINPUT.PAS
KMINPUT.TPP: KMINPUT.PAS
        bpc /b /cp KMINPUT.PAS

DBB.TPU: DBB.PAS SYSTEM2.PAS
        tpc /b DBB.PAS
DBB.TPP: DBB.PAS SYSTEM2.PAS
        bpc /b /cp DBB.PAS

READARC.TPU: READARC.PAS SYSTEM2.PAS
        tpc /b READARC.PAS
READARC.TPP: READARC.PAS SYSTEM2.PAS
        bpc /b /cp READARC.PAS

clean:
        del *.TPU
        del *.TPP
        del *.BAK
install: all
        copy *.tpu ..\TPU\
        copy *.tpp ..\TPU\

