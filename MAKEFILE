all : DOS DPMI

DOS: SYSTEM2.TPU WINCB.TPU STRBIN.TPU ARGS.TPU
DPMI: SYSTEM2.TPP WINCB.TPP STRBIN.TPP ARGS.TPP

SYSTEM2.TPU: SYSTEM2.PAS SYSEMS.PAS SYSXMS.PAS SYSMEM.PAS
	tpc /m SYSTEM2.PAS
SYSTEM2.TPP: SYSTEM2.PAS SYSMEM.PAS
	bpc /m /cp SYSTEM2.PAS

WINCB.TPU: WINCB.PAS
	tpc /m WINCB.PAS
WINCB.TPP: WINCB.PAS
	bpc /m /cp WINCB.PAS

ARGS.TPU: ARGS.PAS
	tpc /m ARGS.PAS
ARGS.TPP: ARGS.PAS
	bpc /m /cp ARGS.PAS

STRBIN.TPU: STRBIN.PAS
	tpc /m STRBIN.PAS
STRBIN.TPP: STRBIN.PAS
	bpc /m /cp STRBIN.PAS

clean:
	del *.TPU
	del *.TPP
